<script>
	var myDate = new Date();
	var hrs = myDate.getHours();
	var greet;
	var color;
	var quote;
	if (hrs < 12) {
		greet = 'Good morning, a200796a <img src="images/park.png"/>';
		greet = 'Good morning, <%=user.name%> ';
		color = '#7FC583';
		quote = 'Every new day is a chance to change your life.';
	} else if (hrs >= 12 && hrs <= 18) {
		greet = 'Good afternoon, <%=user.name%> ';
		greet = 'Good afternoon, <%=user.name%> <img src="images/desert.png"/>';
		color = '#ffc107';
		quote = 'May this afternoon be light, blessed, enlightened, productive and happy.';
	} else if (hrs >= 18 && hrs <= 24) {
		greet = 'Good evening, <%=user.name%> ';
		greet = 'Good evening, <%=user.name%> <img src="images/sea.png"/>';
		color = '#FF4F70';
		quote = 'Evenings are life’s way of saying that you are closer to your dreams.';
	}
	setTimeout(function () {
		$('.greetalert').css('border-left', '2.5px solid ' + color );
		$('#daynightmsg').html('<div class="small-texts">' + greet + '</div><p>' + quote + '</p>');
		$('.greetalert').removeClass('hidden');
	}, 100);

</script>

